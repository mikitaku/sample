<h1>Blogposts#index</h1>
<p>＊投稿記事の一覧</p>
<p><%= @test %></p>

<table>
<tr>
  <th>Id</th><th>Title</th><th>read</th><th>genre</th><th colspan="2"></th>
</tr>

<% @data.each do |obj| %>
  <tr>
    <td><%= obj.id %></td>
    <td><%= obj.title %></td>
    <td><%= obj.read %></td>
    <td><%= obj.bloggenre.name %></td>
    <td><a href="/blogposts/edit/<%= obj.id %>">edit</a></td>
    <td><a href="/blogposts/delete/<%= obj.id %>">delete</a></td>
  </tr>
<% end %>
</table>

<table>
<tr>
  <th>Id</th><th>Name</th><th>Memo</th>
</tr>

<% @genre.each do |ob| %>
  <tr>
    <td><%= ob.id %></td>
    <td><%= ob.name %></td>
    <td><%= ob.memo %></td>
  </tr>
<% end %>
</table>


<a href="/blogposts/add/">新規作成＞＞</a>

